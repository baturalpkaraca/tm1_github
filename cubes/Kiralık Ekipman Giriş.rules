[  ] = IF(ELPAR('Version', !Version, 1) @= 'Aktif', CONTINUE, STET);

['MM Name'] = S: DB( '}ElementAttributes_Masraf_Kar_Merkezleri', DB( 'Kiralık Ekipman Giriş', !Records, !Version, 'MM'  ), 'Name'  );

[ '!!!Uyarı!!!' ] =  S: 
	IF(SUBST(DB( 'Kiralık Ekipman Giriş', !Records, !Version, 'Sözleşme Bitiş Tarihi'), 5, 2) @= SUBST(SUBST ( !Version, LONG ( !Version ) -4 +1, 4), 3, 2), 'Sözleşme Bitişi, bütçe yılı ile aynı yıl içindedir.', CONTINUE);

[ 'Yıllık Kira Maliyeti' ] =  N: 
