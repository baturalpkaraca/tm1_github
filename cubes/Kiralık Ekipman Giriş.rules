[ 'Yeni Ekipman Aylık Kira Bedeli' ] =  N: 
	IF(SUBST(DB( 'Kiralık Ekipman Giriş', !Records, !Version, 'Sözleşme Bitiş Tarihi'), 5, 2) @<> SUBST(SUBST ( !Version, LONG ( !Version ) -4 +1, 4), 3, 2), 0, CONTINUE);

[ 'Para Birimi Yeni' ] =  S: 
IF(SUBST(DB( 'Kiralık Ekipman Giriş', !Records, !Version, 'Sözleşme Bitiş Tarihi'), 5, 2) @<> SUBST(SUBST ( !Version, LONG ( !Version ) -4 +1, 4), 3, 2), '', CONTINUE);

