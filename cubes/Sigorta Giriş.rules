FEEDSTRINGS;
SKIPCHECK;

[] = IF( ELPAR( 'Version', !Version, 1 ) @= 'Aktif', CONTINUE, STET );

['MM'] = S: IF(DB('Sigorta Giriş', !Records, !Version, 'Dağıtım Anahtarı') @<> 'MM', '', CONTINUE);

['MM Name'] = S: DB( '}ElementAttributes_Masraf_Kar_Merkezleri', DB( 'Sigorta Giriş', !Records, !Version, 'MM'  ), 'Name'  );

['Final Prim'] = N: IF(DB('Sigorta Giriş', !Records, !Version, '% Artış') = 0, ['Prim'], ['Prim'] * (1 + ['% Artış']));

FEEDERS;

['Aktif', 'Prim'] => ['Final Prim'];

['Aktif', 'MM'] => ['MM Name'];

