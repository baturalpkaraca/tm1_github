SKIPCHECK;
FEEDSTRINGS;

[Scenario:'Scenario':'Live']=C:IF(!Cash_Flow_M@<>'Amount' & !Cash_Flow_M@<>'Line'  ,0,CONTINUE);

[Scenario:'Scenario':'Live',Production_Type:'Production_Type':'Old',Cash_Flow_M:'Cash_Flow_M':'Spread']=N:
        [Cash_Flow_M:'Cash_Flow_M':'Client_Rate']-[Cash_Flow_M:'Cash_Flow_M':'FTP_Rate'];

[Scenario:'Scenario':'Live',Cash_Flow_M:'Cash_Flow_M':'Client_Amount',Production_Type:'Production_Type':'Old']=N:
        [Cash_Flow_M:'Cash_Flow_M':'Client_Rate'] * [Cash_Flow_M:'Cash_Flow_M':'Amount'];

[Scenario:'Scenario':'Live',Cash_Flow_M:'Cash_Flow_M':'FTP_Amount',Production_Type:'Production_Type':'Old']=N:
        [Cash_Flow_M:'Cash_Flow_M':'FTP_Rate'] * [Cash_Flow_M:'Cash_Flow_M':'Amount'];

FEEDERS;

[Scenario:'Scenario':'Live',Cash_Flow_M:'Cash_Flow_M':'Client_Rate',Production_Type:'Production_Type':'Old']=>
      [Cash_Flow_M:'Cash_Flow_M':'Spread'];

[Scenario:'Scenario':'Live',Cash_Flow_M:'Cash_Flow_M':'Amount',Production_Type:'Production_Type':'Old']=>
      [Cash_Flow_M:'Cash_Flow_M':'Client_Amount'], [Cash_Flow_M:'Cash_Flow_M':'FTP_Amount'];
