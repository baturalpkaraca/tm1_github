FEEDSTRINGS;
SKIPCHECK;

['Fiili', 'Yön (Karşıt Hesap)'] = S: IF(DB('Kanuni Karşılık Giriş', !Records, !Version_Actual, !Period, 'Karşıt Hesap') @<> '',
                                        IF(DB('Kanuni Karşılık Giriş', !Records, !Version_Actual, !Period, 'Yön') @= 'BORÇ','ALACAK',
                                        IF(DB('Kanuni Karşılık Giriş', !Records, !Version_Actual, !Period, 'Yön') @= 'ALACAK', 'BORÇ', CONTINUE)), CONTINUE);

['Fiili', 'Borç Tutar'] = N: IF(DB('Kanuni Karşılık Giriş', !Records, !Version_Actual, !Period, 'Yön') @= 'BORÇ' %
                                DB('Kanuni Karşılık Giriş', !Records, !Version_Actual, !Period, 'Yön (Karşıt Hesap)') @= 'BORÇ', ['Tutar'], CONTINUE);

['Fiili', 'Alacak Tutar'] = N: IF(DB('Kanuni Karşılık Giriş', !Records, !Version_Actual, !Period, 'Yön') @= 'ALACAK'%
                                  DB('Kanuni Karşılık Giriş', !Records, !Version_Actual, !Period, 'Yön (Karşıt Hesap)') @= 'ALACAK', ['Tutar'], 0);

FEEDERS;

['Fiili', 'Yön'] => ['Yön (Karşıt Hesap)'];

['Fiili', 'Tutar'] => ['Borç Tutar'], ['Alacak Tutar'];

#Kanuni Karşılık Küpü için Feederlar.

['Fiili', 'Borç Tutar'] => DB( 'Kanuni Karşılık', 'Toplam', !Records, !Version_Actual, !Period, 'Borç Tutar'  );

['Fiili', 'Alacak Tutar'] => DB( 'Kanuni Karşılık', 'Toplam', !Records, !Version_Actual, !Period, 'Alacak Tutar'  );