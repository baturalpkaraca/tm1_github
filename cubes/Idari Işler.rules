SKIPCHECK;

[  ] = IF(ELPAR('Version', !Version, 1) @= 'Aktif', CONTINUE, STET);

#Personel Sayısı
[ 'Personel Sayısı', 'Tutar' ] =  N: DB( 'IK - Genel Giriş', !Masraf_Kar_Merkezleri, 'TOPLAM', 'TOPLAM', !Version, !Period, 'Çalışan Sayısı'  );

#Yemek
[ 'Yemek', 'Tutar' ] =  N: (((['Personel Sayısı'] + ['Taşeron Personel Sayısı'] - ['Setcard Kullanan Kişi Sayısı'])) * 0 * ['Çalışılan Gün Sayısı']) +
    (['Setcard Kullanan Kişi Sayısı'] * ['Yemek Birim Fiyat']*['Çalışılan Gün Sayısı']);

#Temizlik
[ 'Temizlik', 'Tutar' ] =  N: 
	[ 'Vasıfsız Temizlik Personel Sayısı' ] * ([ 'Vasıfsız Temizlik Birim Fiyat' ] + [ 'Vasıfsız Temizlik Mesai Birim Fiyat' ]) + [ 'Şef Temizlik Personel Sayısı' ] * 
    ([ 'Şef Temizlik Birim Fiyat' ] + [ 'Şef Temizlik Mesai Birim Fiyat' ]) + [ 'Bölge Müdürü Fark Tutarı (Temizlik)' ];

#Hammaliye
[ 'Hammaliye', 'Tutar' ] =  N: 
	[ 'Şef Hammaliye Personel Sayısı' ] * ([ 'Şef Hammaliye Birim Fiyat' ] + [ 'Şef Hammaliye Mesai Birim Fiyat' ]) + [ 'Vasıfsız Hammaliye Personel Sayısı' ] * 
    ([ 'Vasıfsız Hammaliye Birim Fiyat' ] + [ 'Vasıfsız Hammaliye Mesai Birim Fiyat' ]) + [ 'Forklift Hammaliye Personel Sayısı' ] * 
    ([ 'Forklift Hammaliye Birim Fiyat' ] + [ 'Forklift Hammaliye Mesai Birim Fiyat' ]) + [ 'Bölge Müdürü Fark Tutarı (Hammaliye)' ];

#Güvenlik
[ 'Güvenlik', 'Tutar' ] =  N: 
	[ 'Şef Güvenlik Personel Sayısı' ] * ([ 'Şef Güvenlik Birim Fiyat' ] + [ 'Şef Güvenlik Mesai Birim Fiyat' ]) + [ 'Vasıfsız Güvenlik Personel Sayısı' ] * 
    ([ 'Vasıfsız Güvenlik Birim Fiyat' ] + [ 'Vasıfsız Güvenlik Mesai Birim Fiyat' ]);

FEEDERS;

