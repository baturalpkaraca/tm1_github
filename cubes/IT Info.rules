(['Toplam Fiyat (Original Currency)'] \ ['Toplam Adet']) * ['Ağustos'] * DB('V_P', !Version, SUBST ( !Version, LONG ( !Version) -4+1,4)|'-08', 'Aysonu Kur - '|DB( 'IT Info', !Records, !Version, 'Para Birimi'))+
(['Toplam Fiyat (Original Currency)'] \ ['Toplam Adet']) * ['Eylül'] * DB('V_P', !Version, SUBST ( !Version, LONG ( !Version) -4+1,4)|'-09', 'Aysonu Kur - '|DB( 'IT Info', !Records, !Version, 'Para Birimi'))+
