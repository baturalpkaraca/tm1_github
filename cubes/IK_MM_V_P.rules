SKIPCHECK;

[] = IF( ELPAR( 'Version', !Version, 1 ) @= 'Aktif', CONTINUE, STET );

['Eğitim Dağıtım Hesaplama'] = N: IF( DB( 'IK_MM_V_P', !Masraf_Kar_Merkezleri, !Version, !Period, 'Eğitim Dağıtım'  ) @= 'Evet',
			        DB( 'IK - Genel Giriş', !Masraf_Kar_Merkezleri, 'TOPLAM', 'TOPLAM', !Version, !Period, 'Çalışan Sayısı'  ), 0);

['Çay Fişi'] = N: DB( 'IK - Genel Parametreler', 'Çay Fişi Birim Fiyat', !Version, 'Tutar'  ) *
		 (DB( 'IK - Genel Giriş', !Masraf_Kar_Merkezleri, 'Toplam Merkez Unvan', 'TOPLAM', !Version, !Period, 'Çalışan Sayısı'  ) - 
          DB( 'IK - Genel Giriş', !Masraf_Kar_Merkezleri, 'Stajyer Merkez', 'TOPLAM', !Version, !Period, 'Çalışan Sayısı'  ));

['Yılbaşı Yemeği'] = N: DB( 'IK_MM_V_P', !Masraf_Kar_Merkezleri, !Version, !Period, 'Yılbaşı Yemeği Birim Fiyat'  ) * 
          DB( 'IK - Genel Giriş', !Masraf_Kar_Merkezleri, 'TOPLAM', 'TOPLAM', !Version, !Period, 'Çalışan Sayısı'  );

['Mağazalar Sosyal Aktivite ve Taşınma Masrafları'] = N: DB( 'IK_MM_V_P', !Masraf_Kar_Merkezleri, !Version, !Period, 'Mağazalar Sosyal Aktivite ve Taşınma Masrafları Birim Fiyat'  ) *
          DB( 'IK - Genel Giriş', !Masraf_Kar_Merkezleri, 'TOPLAM (Stajyersiz)', 'TOPLAM', !Version, !Period, 'Çalışan Sayısı'  );

FEEDERS;

