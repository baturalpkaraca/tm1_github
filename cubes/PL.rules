SKIPCHECK;

[Scenario:'Scenario':'Live',pl_m:'pl_m':'Amount'] =N:
    IF( DB('System_Version', !Scenario, !Version, !Entity, !Period, 'Input' ) @='Actual',
          [Scenario:'Scenario':'Actual'], 
          CONTINUE
      );

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':{'Commission Income','Other Income','Risk Costs'}]=N:
    IF( !Currency_Reporting @<>'ORJ',
          [Currency_Reporting:'Currency_Reporting':'ORJ'] * DB('Cross Currency', !Scenario, !Version, !Period, !Currency, !Currency_Reporting ),
          CONTINUE
        );

[Currency_Reporting:'Currency_Reporting':'ORJ',PL_Accounts:'PL_Accounts':{'Commission Income','Other Income','Risk Costs'}] = N:STET;
   
[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Net Interest Income']=N:
      DB( 'BS_Treasury', !Scenario, !Version, !Entity, !Period, 'Treasury_Accounts':'Total_Treasury_Accounts', 'Segment':'Total Segment', 'Product':'Total_Product', !Currency, !Currency_Reporting, 'BS_Treasury_M':'NII' );

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Staff Expense']=N:
      DB( 'Expense', !Scenario, !Version, !Entity, !Period, 'Cost_Center':'Total Cost Center', 'Expense_Accounts':'Personel Giderleri', !Currency, !Currency_Reporting, 'Amount' );

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'IT Expenses']=N:
      DB( 'Expense', !Scenario, !Version, !Entity, !Period, 'Cost_Center':'Total Cost Center','External Service Expenses', !Currency, !Currency_Reporting, 'Amount' );

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Office and General Administrative']=N:
      DB( 'Expense', !Scenario, !Version, !Entity, !Period, 'Cost_Center':'Total Cost Center','Office and General Administrative Expenses', !Currency, !Currency_Reporting, 'Amount' );

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Other Expenses']=N:
        DB( 'Expense', !Scenario, !Version, !Entity, !Period, 'Cost_Center':'Total Cost Center', 'Toplam Giderler', !Currency, !Currency_Reporting, 'Amount' )
        -DB( 'Expense', !Scenario, !Version, !Entity, !Period, 'Cost_Center':'Total Cost Center', 'Expense_Accounts':'Personel Giderleri', !Currency, !Currency_Reporting, 'Amount' )
        -DB( 'Expense', !Scenario, !Version, !Entity, !Period, 'Cost_Center':'Total Cost Center','External Service Expenses', !Currency, !Currency_Reporting, 'Amount' )
        -DB( 'Expense', !Scenario, !Version, !Entity, !Period, 'Cost_Center':'Total Cost Center','Office and General Administrative Expenses', !Currency, !Currency_Reporting, 'Amount' );

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Corporate tax']=N:[PL_Accounts:'PL_Accounts':'Result Before Tax'] * 30 \ 100; 

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Depreciation and Amortization']=N:DB( 'Depreciation', !Scenario, !Version, !Entity, !Period, 'Cost_Center':'Total Cost Center', 'Items':'Total_Items', 'Amount' );


FEEDERS;
[Currency_Reporting:'Currency_Reporting':'ORJ',PL_Accounts:'PL_Accounts':{'Commission Income','Other Income'}] =>
   [Currency_Reporting:'Currency_Reporting':'Total Currency'];

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Net Interest Income']=>
      [PL_Accounts:'PL_Accounts':'Total Expenses'],[PL_Accounts:'PL_Accounts':'Corporate tax'];

[Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Net Interest Income',Currency:'Currency':'TRY']=>
    [Scenario:'Scenario':'Live',PL_Accounts:'PL_Accounts':'Net Interest Income',Currency:'Currency':'Total Currency'];

[Scenario:'Scenario':'Actual'] => 
      [Scenario:'Scenario':'Live'];
  