SKIPCHECK;
FEEDSTRINGS;

[  ] = IF(ELPAR('Version', !Version, 1) @= 'Aktif', CONTINUE, STET);

['MM Name'] = S: DB( '}ElementAttributes_Masraf_Kar_Merkezleri', DB( 'Kiralık Araç Giriş', !Records, !Version, 'MM'  ), 'Name'  );

[ '!!!Uyarı!!!' ] =  S: 
	IF(SUBST(DB('Kiralık Araç Giriş', !Records, !Version, 'Sözleşme Bitimi'), 5, 2) @= SUBST(SUBST ( !Version, LONG ( !Version ) -4 +1, 4), 3, 2), 'Sözleşme Bitişi, bütçe yılı ile aynı yıl içindedir.', CONTINUE);

[ 'Yıllık Kira Maliyeti' ] =  N: 
	DB('Kiralık Araç (Hesap)', DB('Kiralık Araç Giriş', !Records, !Version, 'MM'), !Records, !Version, SUBST ( !Version, LONG ( !Version ) -4 +1, 4), 'Kiralama Bedeli');

[ 'Yıllık Yakıt Maliyeti' ] =  N: 
	DB('Kiralık Araç (Hesap)', DB('Kiralık Araç Giriş', !Records, !Version, 'MM'), !Records, !Version, SUBST ( !Version, LONG ( !Version ) -4 +1, 4), 'Yakıt Maliyeti');

[ 'Yeni Araç Aylık Kira Bedeli' ] =  N: 
	IF(SUBST(DB('Kiralık Araç Giriş', !Records, !Version, 'Sözleşme Bitimi'), 5, 2) @<> SUBST(SUBST ( !Version, LONG ( !Version ) -4 +1, 4), 3, 2), 0, CONTINUE);

[ 'Para Birimi Yeni' ] =  S: 
	IF(SUBST(DB('Kiralık Araç Giriş', !Records, !Version, 'Sözleşme Bitimi'), 5, 2) @<> SUBST(SUBST ( !Version, LONG ( !Version ) -4 +1, 4), 3, 2), '', CONTINUE);

FEEDERS;

