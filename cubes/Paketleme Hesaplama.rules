SKIPCHECK;

[  ] = IF(ELPAR('Version', !Version, 1) @= 'Aktif', CONTINUE, STET);

['Amount'] = N: IF(DB('V_P', !Version, !Period, 'Actual - Plan') @= 'Actual', STET, CONTINUE);

[ 'Amount' ] =  N: 
	IF((ELISANC('Masraf_Kar_Merkezleri', 'Koçtaş', !Masraf_Kar_Merkezleri) = 1
	% ELISANC('Masraf_Kar_Merkezleri', 'Fix', !Masraf_Kar_Merkezleri) = 1), 
    (DB('Paketleme Giriş', 'Total Records', !Version, 'Kiosk Tutarı (TL)') \ 
    (DB('Satış', 'Koçtaş', !Version, SUBST(!Version, LONG(!Version)-4+1, 4), 'Kiosk Yarı Gross Ciro') + 
    DB('Satış', 'Fix', !Version, SUBST(!Version, LONG(!Version)-4+1, 4), 'Kiosk Yarı Gross Ciro')) * 
    DB( 'Satış', !Masraf_Kar_Merkezleri, !Version, !Period, 'Kiosk Yarı Gross Ciro'  )), CONTINUE);

# [ 'Amount' ] =  N: 
# 	IF(!Masraf_Kar_Merkezleri @= '5340000', 
#     DB('Paketleme Giriş', 'Total Records', !Version, 'Web Tutarı (TL)') * 
#     (DB('Satış', '5340000', !Version, !Period, 'Net Ciro') \ 
#     DB('Satış', '5340000', !Version, SUBST(!Version, LONG(!Version)-4+1, 4), 'Net Ciro')), CONTINUE);

[ 'Amount' ] =  N: 
	IF(ELISANC('Masraf_Kar_Merkezleri', 'B2C', !Masraf_Kar_Merkezleri) = 1, 
    DB('Paketleme Giriş', 'Total Records', !Version, 'Web Tutarı (TL)') * 
    (DB('Satış', !Masraf_Kar_Merkezleri, !Version, !Period, 'Net Ciro') \ 
    DB('Satış', 'B2C', !Version, SUBST(!Version, LONG(!Version)-4+1, 4), 'Net Ciro')), CONTINUE);

[ 'Amount' ] =  N: 
	IF(!Masraf_Kar_Merkezleri @= '3340002', 
    DB('Paketleme Giriş', 'Total Records', !Version, 'Fix Depo Tutarı (TL)') * 
    (DB('Satış', 'Fix', !Version, !Period, 'Net Ciro') \ 
    DB('Satış', 'Fix', !Version, SUBST(!Version, LONG(!Version)-4+1, 4), 'Net Ciro')), CONTINUE);

FEEDERS;

['Aktif', 'Amount' ] => DB('Genel Giderler', '8180900001', !Masraf_Kar_Merkezleri, 'Lojistik', !Version, !Period, 'Hesaplanan Tutar');
