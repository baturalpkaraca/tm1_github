elseif (Final_External_Satis_Meas @= 'Mobil Ticari');
    vMargin = CellGetN( 'MG_SK_P', MalGrubu, 'Mobil Ticari', pVersiyon, SUBST( Day, 1, 7 ), 'Ticari Margin' );
