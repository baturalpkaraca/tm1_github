    CellPutN( Value * vOran, 'Final External Satis', MalGrubu, Masraf_Kar_Merkezleri, Day, pVersiyon, 'Mobil Ticari Margin' );
elseif (Final_External_Satis_Meas @= 'Ticari Satış');
