#Mağaza elemanından Satış Kanalı bilgisi elde edilir.
vMasrafMerkezi = ATTRS( 'Masraf_Kar_Merkezleri', Masraf_Kar_Merkezleri, 'Kar Merkezi Code' );

IF(Final_External_Satis_Meas @= 'Perakende Satış' % Final_External_Satis_Meas @= '3P Satış');

    IF(SUBST(vMasrafMerkezi,1,1) @= '1' % SUBST(vMasrafMerkezi,1,1) @= '2');
        SatisKanali = 'Bigbox'; 
    ELSEIF(SUBST(vMasrafMerkezi,1,1) @= '6');
        SatisKanali = 'Fix'; 
    ELSEIF(vMasrafMerkezi @= '5001');
        SatisKanali = 'Web';
    ELSEIF(vMasrafMerkezi @= '5002');
        SatisKanali = 'B2B - 5002 Projeler';
    ENDIF;

ELSE;

    IF(SUBST(vMasrafMerkezi,1,1) @= '1' % SUBST(vMasrafMerkezi,1,1) @= '2');
        SatisKanali = 'Bigbox'; 
    ELSEIF(vMasrafMerkezi @= '5001');
        SatisKanali = 'Web';
    ELSEIF(vMasrafMerkezi @= '5002');
        SatisKanali = 'B2B - 5002 Projeler';
    ELSEIF(SUBST(vMasrafMerkezi,1,1) @= '4' % SUBST(vMasrafMerkezi,1,1) @= '5');
        SatisKanali = 'B2B'; 
    ELSEIF(SUBST(vMasrafMerkezi,1,1) @= '6');
        SatisKanali = 'Fix'; 
    ELSEIF(vMasrafMerkezi @= '8002');
        SatisKanali = 'Mobil Ticari';
    ENDIF;

ENDIF;

if (Final_External_Satis_Meas @= 'Perakende Satış'); 
    vMargin = CellGetN( 'MG_SK_P', MalGrubu, SatisKanali, pVersiyon, SUBST( Day, 1, 7 ), 'Perakende Margin' );
    vSatis = CellGetN( 'MG_SK_P', MalGrubu, SatisKanali, pVersiyon, SUBST( Day, 1, 7 ), 'Perakende Satış' );
    vOran = vMargin \ vSatis;
    CellPutN( Value * vOran, 'Final External Satis', MalGrubu, Masraf_Kar_Merkezleri, Day, pVersiyon, 'Perakende Margin' );
elseif (Final_External_Satis_Meas @= 'Handyman Sales');
    vMargin = CellGetN( 'MG_SK_P', MalGrubu, SatisKanali, pVersiyon, SUBST( Day, 1, 7 ), 'Handyman Margin' );
    vSatis = CellGetN( 'MG_SK_P', MalGrubu, SatisKanali, pVersiyon, SUBST( Day, 1, 7 ), 'Handyman Sales' );
