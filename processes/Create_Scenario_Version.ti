#region Prolog
IF(pSelection@='Scenario');
    IF( DimensionElementExists( 'Scenario', pName ) = 0 );  
        DimensionElementInsertDirect( 'Scenario', '', pName, 'N' );
    ENDIF;
ENDIF;

IF(pSelection@='Version');
    IF( DimensionElementExists( 'Version', pName ) = 0 );  
        DimensionElementInsertDirect( 'Version', '', pName, 'N' );
    ENDIF;
ENDIF;
#endregion